# TruthShield API Keys

# Perplexity API Key for fact-checking (required)
# Get it from: https://www.perplexity.ai/settings/api
REACT_APP_PERPLEXITY_API_KEY=your_perplexity_api_key_here

# Google Gemini API Key for OCR and text extraction (required)
# Get it from: https://aistudio.google.com/app/apikey
REACT_APP_GEMINI_API_KEY=your_gemini_api_key_here

# Analytics settings (optional)
REACT_APP_ANALYTICS_ENABLED=false
REACT_APP_ANALYTICS_ENDPOINT=https://your-analytics-endpoint.com 